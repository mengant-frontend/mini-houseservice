page {
  /* 给 scroll-loading 组件继承高度 */
  height: 100%;
	background-color: #F3F3F3;
}
.navigator-hover{
	background-color: #fff;
}
.bar {
  position: fixed;
  right: 25rpx;
  bottom: 170rpx;
}

.navigator{
  box-shadow: 0 1rpx 2rpx 0;
  border-radius: 8rpx;
  padding: 15rpx;
  background: #fff;
}
.navigator text {
  color: #ff9900;
}

.block__nav{
	width: 750rpx;
	padding: 20rpx 30rpx;
	display: flex;
	flex-direction: row;
	background-color: #fff;
	box-sizing: border-box;
}
.block__nav-item{
	flex-grow: 1;
	flex-shrink: 0;
	display: flex;
	flex-direction: row;
	align-items: center;
}
.left__nav{
	padding-right: 78rpx;
	border-right: 1px solid #dcdcdc;
}
.right__nav{
	padding-left: 78rpx;
}
.block__icon{
	width: 40rpx;
	height: 40rpx;
	display: flex;
	justify-content: center;
	align-items: center;
	flex-shrink: 0;
	border-radius: 10rpx;
}
.title__nav{
	flex-grow: 1;
	flex-shrink: 0;
	font-size: 28rpx;
	color: #2a2a2a;
}
.enter__nav{
	flex-shrink: 0;
}
.demand-hall{
	background-color: #ea819f;
}
.price-guide{
	background-color: #f49359;
}
.title__nav{
	margin-left: 12rpx;
}
.main{
	margin-top: 18rpx;
	
}