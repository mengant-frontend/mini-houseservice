page {
  padding: 25rpx 25rpx;
  box-sizing: border-box;
}
.page {
  padding-bottom: 64px;
}
.container {
  width: 100%;
  min-height: 100%;
}
.ellipsis {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.container-title {
  text-align: left;
  font-size: 16px;
  color: #1c2438;
  font-weight: 600;
  margin-bottom: 25rpx;
}
.title {
  font-size: 13px;
  color: #495060;
  text-align: right;
  vertical-align: top;
  white-space: nowrap;
  padding-bottom: 25rpx;
}
.title::after {
  content: ':';
  padding-right: 20rpx;
}
.content {
  font-size: 14px;
  color: #1c2438;
  padding-bottom: 25rpx;
}
.container__img {
  width: 468.75rpx;
  vertical-align: text-bottom;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
}
.square {
  width: 156.25rpx;
  height: 156.25rpx;
  box-sizing: content-box;
  padding: 0 25rpx 25rpx 0;
}
image {
  width: 100%;
  height: 100%;
  border-radius: 4px;
}
