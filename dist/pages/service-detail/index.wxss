page {
  background-color: #EFEFF4;
  padding-bottom: 85rpx;
}
.service {
  padding: 30rpx;
  background-color: #fff;
  border-bottom: 1rpx solid #F3F3F3;
}
.service-name {
  font-size: 33rpx;
  color: #2d2d2d;
  padding-top: 12rpx;
}
.service-desc {
  font-size: 26rpx;
  color: #949495;
  padding-top: 12rpx;
}
.service-price {
  padding-top: 30rpx;
  font-size: 38rpx;
  color: #f49940;
}
.message-list {
  margin-bottom: 20rpx;
}
.message-item {
  display: flex;
  flex-direction: row;
  align-items: center;
  padding: 30rpx;
  background-color: #fff;
  border-bottom: 1rpx solid #F3F3F3;
}
.message-item .left {
  flex-grow: 1;
  flex-shrink: 1;
}
.message-item .right {
  flex-grow: 0;
  flex-shrink: 0;
}
.location__text {
  margin-left: 30rpx;
}
.phone {
  padding-left: 30rpx;
  border-left: 1rpx solid #dcdcdc;
}
.icon-phone {
  width: 39rpx;
  height: 39rpx;
}
.card {
  background-color: #fff;
  margin-bottom: 0;
}
.card-title {
  padding: 35rpx 30rpx;
  font-size: 29rpx;
  color: #2d2d2d;
  letter-spacing: 0px;
}
.menu-bar {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 105rpx;
  display: flex;
  flex-direction: row;
}
.store,
.collect {
  width: 210rpx;
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  background-color: #fff;
  font-size: 24rpx;
}
.book__btn {
  height: 100%;
  flex-grow: 1;
  flex-shrink: 1;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: #2fb97d;
  color: #fff;
  font-size: 38rpx;
}
.extend_price {
  font-size: 26rpx;
  color: #949495;
}
