page {
  background: #f8f8f9;
}
.page {
  padding: 25rpx 25rpx;
}
.panel-title {
  position: relative;
  top: 0;
  left: 0;
  font-size: 14px;
  line-height: 1;
  color: #1c2438;
  padding: 20px 16px 10px;
  background: rgba(255, 255, 255, 0.5);
}
.money {
  font-weight: 600;
  padding-left: 30rpx;
}
.state {
  position: absolute;
  top: 20px;
  right: 16px;
  transform: rotate(10deg);
  font-size: 12px;
  padding: 3px 6px;
  font-weight: normal;
}
.success {
  border: 1px solid #19be6b;
  color: #19be6b;
}
.process {
  border: 1px solid #5cadff;
  color: #5cadff;
}
.fail {
  border: 1px solid #ed3f14;
  color: #ed3f14;
}
