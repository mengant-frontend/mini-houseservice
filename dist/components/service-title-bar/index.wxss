.container {
  position: relative;
  left: 0;
  top: 0;
  width: 100%;
}
.center {
  width: 202rpx;
  margin: 25rpx auto;
  display: flex;
  flex-direction: row;
}
.line-container {
  display: flex;
  justify-content: center;
  align-items: center;
  flex-grow: 1;
  flex-shrink: 1;
}
.line {
  width: 100%;
  height: 2rpx;
  background: #ababab;
}
.title {
  padding: 0 10rpx;
  font-size: 32rpx;
  color: #2d2d2d;
}
.more {
  position: absolute;
  right: 30rpx;
  top: 50%;
  font-size: 20rpx;
  color: #bdbdbd;
  height: 40rpx;
  line-height: 40rpx;
  margin-top: -20rpx;
  /* margin-top: -50%; */
}
